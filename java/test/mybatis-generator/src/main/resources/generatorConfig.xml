<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
         PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
         "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="table_country" targetRuntime="MyBatis3">
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://192.168.137.8:3306/mybatis?useSSL=false" userId="root" password="123456"></jdbcConnection>
        <javaModelGenerator targetPackage="com.gerrardliu.test.hello.model" targetProject="out"></javaModelGenerator>
        <sqlMapGenerator targetPackage="com.gerrardliu.test.hello.mapper" targetProject="out"></sqlMapGenerator>
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.gerrardliu.test.hello.dao" targetProject="out"></javaClientGenerator>
        <table tableName="country" domainObjectName="Country"></table>
    </context>
</generatorConfiguration>